#!/bin/bash
export CLIPBOARD="pbcopy -Prefer txt"
export PASTEBOARD="pbpaste"
export CLICOLOR=1
export LSCOLORS="gxfxcxdxbxegedabagacad"

if [ -x /usr/local/bin/ls ]; then
	alias ls='/usr/local/bin/ls -hF --color=auto'
else
	alias ls='ls -hFG'
fi

prompt default

